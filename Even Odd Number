public class EvenOdd {
    public static void main(String[] args) {
        int[] numbers = {2, 5, 8, 9, 12, 15};  // Sample array

        int even = 0, odd = 0;

        for (int num : numbers) {
            if (num % 2 == 0) {
                even++;
            } else {
                odd++;
            }
        }

        System.out.println("Even numbers: " + even);
        System.out.println("Odd numbers: " + odd);
    }
}
